{
    "compilerOptions": {
        "jsx": "react",
        "target": "es5",
        "lib": [
            "esnext",
            "esnext.asynciterable",
            "dom"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "outDir": "dist"
    },
    "include": [
        "src/**/*"
    ]
}

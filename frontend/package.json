{
    "name": "visual-dl",
    "version": "2.0.0-beta",
    "title": "Visual DL",
    "description": "A platform to visualize the deep learning process and result.",
    "keywords": [
        "visualdl",
        "paddlepaddle",
        "visualization",
        "deep learning"
    ],
    "author": "PeterPanZH, Niandalu",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon --watch server --ext ts --exec \"ts-node --project=server/tsconfig.json\" server/index.ts",
        "build:next": "next build",
        "build:server": "tsc --project server/tsconfig.json",
        "start": "NODE_ENV=production node server/index.js",
        "lint": "tsc --noEmit && eslint --ext .tsx,.jsx.ts,.js --ignore-path .gitignore .",
        "format": "prettier --write \"**/*.ts\" \"**/*.tsx\" \"**/*.js\"  \"**/*.jsx\"",
        "test": "echo \"Error: no test specified\" && exit 0"
    },
    "dependencies": {
        "echarts": "4.6.0",
        "express": "4.17.1",
        "isomorphic-unfetch": "3.0.0",
        "lodash": "4.17.15",
        "next": "9.2.2",
        "next-i18next": "4.2.0",
        "nprogress": "0.2.0",
        "polished": "3.4.4",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-input-range": "1.3.0",
        "react-is": "16.12.0",
        "styled-components": "5.0.1",
        "swr": "0.1.17"
    },
    "devDependencies": {
        "@babel/core": "7.8.4",
        "@types/echarts": "4.4.3",
        "@types/express": "4.17.2",
        "@types/faker": "4.1.9",
        "@types/lodash": "4.14.149",
        "@types/node": "13.7.4",
        "@types/nprogress": "0.2.0",
        "@types/react": "16.9.22",
        "@types/styled-components": "5.0.0",
        "@types/webpack": "4.41.6",
        "@typescript-eslint/eslint-plugin": "2.20.0",
        "@typescript-eslint/parser": "2.20.0",
        "babel-plugin-styled-components": "1.10.7",
        "babel-plugin-typescript-to-proptypes": "1.2.1",
        "cross-env": "7.0.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-prettier": "3.1.2",
        "eslint-plugin-react": "7.18.3",
        "eslint-plugin-react-hooks": "^2.4.0",
        "faker": "4.1.0",
        "nodemon": "2.0.2",
        "prettier": "1.19.1",
        "ts-node": "8.6.2",
        "typescript": "3.8.2"
    }
}

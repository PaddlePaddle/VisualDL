{
    "name": "visualdl",
    "version": "2.0.0-beta",
    "title": "VisualDL",
    "description": "A platform to visualize the deep learning process and result.",
    "keywords": [
        "visualdl",
        "paddlepaddle",
        "visualization",
        "deep learning"
    ],
    "author": "Peter Pan",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon --watch server --watch nuxt.config.ts --ext ts,json --exec \"ts-node --project=server/tsconfig.json\" server/index.ts",
        "build": "sh scripts/build.sh",
        "build:nuxt": "NODE_ENV=production nuxt-ts build",
        "build:server": "tsc --project server/tsconfig.json",
        "start": "cross-env NODE_ENV=production node server/index.js",
        "generate": "nuxt-ts generate",
        "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore .",
        "test": "jest"
    },
    "lint-staged": {
        "*.{js,vue}": "yarn lint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@nuxt/typescript-runtime": "0.3.8",
        "@nuxtjs/axios": "5.9.5",
        "@nuxtjs/dotenv": "1.4.1",
        "@vue/composition-api": "0.3.4",
        "cross-env": "7.0.0",
        "dotenv": "8.2.0",
        "i18next": "19.1.0",
        "koa": "2.11.0",
        "nuxt": "2.11.0",
        "nuxt-typed-vuex": "0.1.16",
        "styled-components": "5.0.1"
    },
    "devDependencies": {
        "@nuxt/typescript-build": "0.5.6",
        "@nuxtjs/eslint-config-typescript": "1.0.2",
        "@nuxtjs/eslint-module": "1.1.0",
        "@types/dotenv": "8.2.0",
        "@types/koa": "2.11.1",
        "@vue/test-utils": "1.0.0-beta.31",
        "babel-eslint": "10.0.3",
        "babel-jest": "25.1.0",
        "babel-preset-vca-jsx": "0.3.4",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-nuxt": "0.5.1",
        "eslint-plugin-prettier": "3.1.2",
        "husky": "4.2.3",
        "jest": "25.1.0",
        "js-yaml-loader": "1.2.2",
        "lint-staged": "10.0.7",
        "nodemon": "2.0.2",
        "prettier": "1.19.1",
        "ts-node": "8.6.2",
        "typescript": "3.7.5",
        "vue-jest": "4.0.0-beta.2"
    },
    "engines": {
        "node": ">=10.0.0",
        "npm": ">=6"
    }
}
